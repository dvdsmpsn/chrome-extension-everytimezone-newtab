{
  "name": "Every Time Zone New Tab",
  "version": "1",
  "manifest_version": 2,
  "description": "Replaces the default new tab with your bookmarks bar & everytimezone.com.",
  "homepage_url": "http://davidsimpson.me/",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  },
  "permissions": [
    "bookmarks"
  ]
}